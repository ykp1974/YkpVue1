<!DOCTYPE html>
<html lang="ja">
<head>
  <meta charset="UTF-8">
  <title>Vue Router</title>
  <script src="https://unpkg.com/vue/dist/vue.js"></script>
  <script src="https://unpkg.com/vue-router/dist/vue-router.js"></script>
</head>
<body>
	<div id="app">
		<h1>{{ title }}</h1>
	</div>
<script>
	var app = new Vue({
		el: '#app',
		data:{
			title: "Vue Resource"
		}
	});
</script>
</body>
</html>